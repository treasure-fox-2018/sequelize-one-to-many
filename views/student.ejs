<!DOCTYPE html>
<html>
<head>
    <title>Data Student</title>
</head>
<body>
    <h1>Data Student</h1>
    <table border='1'>

        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Option</th>
        </tr>
        <% data.forEach(function(dataStudent){ %>
            <tr>
                <td><%= dataStudent.id%></td>
                <td><%= dataStudent.first_name %></td>
                <td><%= dataStudent.last_name %></td>
                <td><%= dataStudent.email %></td>
                <td>
                    <a href="edit/<%= dataStudent.id%>">Edit</a>
                    ||
                    <a href="delete/<%= dataStudent.id%>">Delete</a>
                </td>
                <% })%>
            </tr>

    </table>
    <br><br>
    <a href="students/add">Add New Student</a>
</body>
</html>