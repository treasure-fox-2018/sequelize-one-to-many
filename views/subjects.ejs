<!DOCTYPE html>
<html>
<head>
<style>
table {
    font-family: arial, sans-serif;
    border-collapse: collapse;
    width: 100%;
}

td, th {
    border: 1px solid #dddddd;
    text-align: left;
    padding: 8px;
}

h2, p {
    text-align: center;
}
</style>
</head>
<body>

<h2>All Subjects Data</h2>
<table>
    <thead>
      <tr>
        <th>Id</th>
        <th>Subject Name</th>
        <th>Teacher</th>
      </tr>
    </thead>
    <tbody>
      <% subjects.forEach(subject => { %>
        <tr>
          <td><%= subject.id %></td>
          <td><%= subject.subjectName %></td>
          <td><ol>
          <% subject.Teachers.forEach(teacher => { %>
            <li>
                <%= teacher.firstName %>
                <%= teacher.lastName %>
            </li>
          <% }) %>
        </ol>
        </td>
        </tr>
      <% })%>
    </tbody>
  </table>
</body>
</html>

