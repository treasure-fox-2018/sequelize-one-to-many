<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>Subject Page</title>
	<link rel="stylesheet" href="">
</head>
<body>
	<h3>Subject Data</h3>
	<br>
	<table border=1>
		<thead>
			<tr>
				<th>ID</th>
                <th>Subject Name</th>
                <th>teacher</th>
			</tr>
		</thead>
		<tbody>
			<% subjects.forEach((subject) => { %>
				<tr>
					<td><%=subject.id%></td>
                    <td><%=subject.subjectName%></td>
                    <td>
                        <ol>
                        <%for(var i = 0; i < subject.Teachers.length; i++){%>
                            <li><%=subject.Teachers[i].firstName%> 
                                <%=subject.Teachers[i].lastName%> </li>
                        <%}%>
                        </ol>
                    </td>
				</tr>
			<% }) %>
		</tbody>
    </table><br>
	<h3><a href="/">Back</a></h3>
</body>
</html>