<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Teacher List</title>

</head>
<body>
    <table>
        <h2>All Subjects Data</h2>
        <tr>
          <th>id</th>
          <th>Subject Name</th>
          <th>Teacher</th>
          
        </tr>
        <% subjects.forEach(function(subject) { %>
            <tr>
                
               <td> <%= subject.id %></td>
               <td> <%= subject.subject_name %></td>
               <td> 
                   <% var count = 1 %>
                    <% subject.Teachers.forEach(function(teacher){ %>
                        
                        <%= `${count++}.` + teacher.first_name %> <%=teacher.last_name %>
                        <br>
                    <% }) %>
               </td>

            </tr>

        <% }) %>
        
      </table>
</body>
</html>