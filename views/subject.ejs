<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Page Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <style>
        table {
            font-family: arial, sans-serif;
            border-collapse: collapse;
            width: 100%;
        }
        
        td, th {
            border: 1px solid #dddddd;
            text-align: left;
            padding: 8px;
        }
        
        tr:nth-child(even) {
            background-color: #dddddd;
        }
        </style>
</head>
<body>
    <h1>DATA SUBJECT</h1>
    <h2><a href="/">home page</a></h2>
    <table>
        <tr>
          <th>id</th>
          <th>subject name</th>
          <th>teacher</th>

        </tr>
        <% dataSubject.forEach(function(subject){%>
        <tr>
          <td><%= subject.id %></td>
          <td><%= subject.subjectName %></td>
          <td>
            <ol>
                <% subject.Teachers.forEach(function(teacher){ %>
                    <li><%= teacher.firstName %> <%= teacher.lastName %> </li>
                <% }) %>
            </ol>
          </td>
        </tr>
        <% }) %>
      </table>
</body>
</html>